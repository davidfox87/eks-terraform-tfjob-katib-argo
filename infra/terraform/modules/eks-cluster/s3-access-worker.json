{
    "Version": "2012-10-17",
    "Statement": [
       {
        "Sid": "Fetch",
        "Effect": "Allow",
        "Action": [
            "s3:GetObject",
            "s3:PutObject",
            "s3:ListBucket",
            "s3:GetBucketLocation"
